<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".SettingActivity">

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintHeight_percent="1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:paddingStart="15dp"
        android:paddingEnd="15dp"
        android:orientation="vertical">


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="120dp"
            android:background="@drawable/remove_add_free_bg">

            <LinearLayout
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:orientation="vertical"
                android:layout_alignParentStart="true"
                android:layout_marginStart="27dp"
                android:layout_centerVertical="true"
                android:layout_marginEnd="8dp"
                android:id="@+id/avter_layout"
                android:layout_marginBottom="8dp">


                <ImageView
                    android:id="@+id/iv_image"
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/user_profile_icon" />

                <TextView
                    android:id="@+id/tv_avtaar"
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:text="GH"
                    android:gravity="center"
                    android:visibility="gone"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_14sdp"
                    android:background="@drawable/circular_avatar_background" />

            </LinearLayout>



      <LinearLayout
          android:layout_width="wrap_content"
          android:layout_height="match_parent"
          android:orientation="vertical"
          android:layout_toEndOf="@id/avter_layout"
          android:gravity="center_vertical"
          android:layout_marginStart="15dp"

          >

          <TextView
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:text="User"
              android:textSize="@dimen/_12ssp"
              android:id="@+id/tv_name"
              android:fontFamily="@font/poppins_semibold"
              android:textColor="@color/toolbar_title_color"
              />


          <TextView
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:visibility="gone"
              android:textSize="@dimen/_9ssp"
              android:id="@+id/tv_email"
              android:fontFamily="@font/poppins_regular"
              android:textColor="@color/filter_text_color" />

          <View
              android:layout_width="match_parent"
              android:layout_height="1dp"
              android:id="@+id/viewLine"
              android:visibility="gone"
              android:layout_below="@id/tv_email"
              android:layout_toEndOf="@id/avter_layout"
              android:background="#FFDDDD"/>


          <TextView
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_below="@id/viewLine"
              android:layout_toEndOf="@id/avter_layout"
              android:text="Login your account"
              android:textSize="@dimen/_9ssp"
              android:id="@+id/loginAccount"
              android:fontFamily="@font/poppins_medium"
              android:textColor="@color/red"
              android:paddingBottom="10dp"/>



      </LinearLayout>

        </RelativeLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="120dp"
            android:orientation="horizontal"
            android:layout_marginTop="8dp"
            android:weightSum="1">


            <RelativeLayout
                android:id="@+id/adsFree"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginEnd="2dp"
                android:layout_weight="0.5"
                android:background="@drawable/remove_add_free_bg">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginStart="12dp"
                    android:layout_marginTop="17dp"
                    android:visibility="gone"
                    android:id="@+id/selectedAddFreePlanShow"
                    android:background="@drawable/any_purchase_bg">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="1.5dp"
                        android:layout_gravity="center_vertical"
                        android:src="@drawable/purchase_checkbox_icon"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Monthly Plan"
                        android:textColor="@color/white"
                        android:fontFamily="@font/poppins_medium"
                        android:textSize="@dimen/_6ssp"
                        android:textAllCaps="true"
                        android:layout_marginEnd="4dp"
                        android:id="@+id/planSetAddFreeTextview"
                        android:layout_marginStart="2dp"/>



                </LinearLayout>

                <TextView
                    android:id="@+id/removeAddTextview"
