                android:src="@drawable/version_icon" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@id/iconAppVerion"
                android:text="@string/app_version"
                android:id="@+id/appVersioin"
                android:textSize="@dimen/_11ssp"
                android:layout_centerVertical="true"
                android:layout_marginStart="8dp"
                android:fontFamily="@font/poppins_medium"
                android:textColor="@color/toolbar_title_color"

                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@id/appVersioin"
                android:text="1.1.1"
                android:id="@+id/appVersioinNumber"
                android:textSize="@dimen/_11ssp"
                android:layout_centerVertical="true"
                android:layout_marginStart="8dp"
                android:fontFamily="@font/poppins_medium"
                android:textColor="@color/red"

                />


            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginStart="15dp"
                android:layout_alignParentBottom="true"
                android:background="@color/bg_color1"/>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_marginTop="5dp"
            android:id="@+id/logoutBtn"
            >

            <ImageView
                android:id="@+id/iconLogout"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_alignParentStart="true"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:layout_marginBottom="8dp"
                android:src="@drawable/logout_icon" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@id/iconLogout"
                android:text="@string/logout"
                android:textSize="@dimen/_11ssp"
                android:layout_centerVertical="true"
                android:layout_marginStart="8dp"
                android:fontFamily="@font/poppins_medium"
                android:textColor="@color/toolbar_title_color"

                />

        </RelativeLayout>

    </LinearLayout>

    </androidx.core.widget.NestedScrollView>

