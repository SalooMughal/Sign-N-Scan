                    android:textColor="@color/toolbar_title_color"
                    android:textSize="@dimen/_13ssp"

                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/removeAddTextview"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="12dp"
                    android:id="@+id/subTitleRemoveAds"
                    android:fontFamily="@font/poppins_medium"
                    android:text="@string/purchase_now"
                    android:textColor="@color/red"
                    android:textSize="@dimen/_9ssp"

                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/subTitleRemoveAds"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="12dp"
                    android:visibility="gone"
                    android:id="@+id/manageSubscrpionInRemoveAdSide"
                    android:fontFamily="@font/poppins_medium"
                    android:text="Manage Subscription"
                    android:layout_marginTop="8dp"
                    android:textColor="@color/red"
                    android:textSize="@dimen/_8ssp"

                    />

                <ImageView
                    android:layout_width="26dp"
                    android:layout_height="22dp"
                    android:layout_alignParentRight="true"
                    android:layout_marginEnd="8dp"
                    android:layout_marginTop="12dp"
                    android:visibility="gone"
                    android:id="@+id/whenAnyAddRemovePurchaseIcon"
                    android:src="@drawable/remove_add_icon"

                    />

                <ImageView
                    android:id="@+id/iconAdsfree"
                    android:layout_width="73dp"
                    android:layout_height="71dp"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginEnd="8dp"
                    android:layout_marginBottom="2dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/remove_add_icon"

                    />


            </RelativeLayout>

            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.5"
                android:layout_marginStart="2dp"
                android:id="@+id/pdfToolsPurchaseBtn"
                android:background="@drawable/remove_add_free_bg"
                >

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginStart="12dp"
                    android:layout_marginTop="17dp"
                    android:visibility="gone"
                    android:id="@+id/selectedPdfToolsPlanShow"
                    android:background="@drawable/any_purchase_bg">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="1.5dp"
                        android:layout_gravity="center_vertical"
                        android:src="@drawable/purchase_checkbox_icon"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Monthly Plan"
                        android:textColor="@color/white"
                        android:fontFamily="@font/poppins_medium"
                        android:textSize="@dimen/_6ssp"
                        android:layout_marginEnd="4dp"
                        android:textAllCaps="true"
                        android:id="@+id/planSetPdfToolsTextview"
                        android:layout_marginStart="2dp"/>



                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/advance_tools"
                    android:id="@+id/advanceToolsTextview"
                    android:textSize="@dimen/_13ssp"
                    android:includeFontPadding="false"
                    android:layout_below="@+id/selectedPdfToolsPlanShow"
                    android:layout_marginTop="5dp"
                    android:layout_marginStart="12dp"
                    android:fontFamily="@font/poppins_semibold"
                    android:textColor="@color/toolbar_title_color"

                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/advanceToolsTextview"
                    android:text="@string/purchase_now"
                    android:textSize="@dimen/_9ssp"
                    android:id="@+id/subTitleToolsPdf"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="12dp"
                    android:fontFamily="@font/poppins_medium"
                    android:textColor="@color/red"

                    />


                <ImageView
                    android:layout_width="21dp"
                    android:layout_height="21dp"
                    android:layout_alignParentRight="true"
                    android:layout_marginEnd="8dp"
                    android:visibility="gone"
                    android:layout_marginTop="12dp"
                    android:id="@+id/whenAnyAdvanceToolsPurchaseIcon"
                    android:src="@drawable/advance_tools_icon"

                    />

                <ImageView
                    android:layout_width="73dp"
                    android:layout_height="60dp"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginEnd="8dp"
                    android:layout_marginBottom="10dp"
                    android:id="@+id/puchaseIcon"
                    android:src="@drawable/advance_tools_icon"

                    />


            </RelativeLayout>



        </LinearLayout>


        <TextView
