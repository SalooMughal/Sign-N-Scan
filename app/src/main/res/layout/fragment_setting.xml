                android:fontFamily="@font/gilroy_medium"
                android:textColor="@color/toolbar_title_color"
                android:layout_centerVertical="true"
                android:layout_marginStart="8dp"/>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
