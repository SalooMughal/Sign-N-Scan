                android:layout_marginEnd="8dp"
                android:layout_marginBottom="8dp"
                android:src="@drawable/current_mimber_ship_icon" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@id/mimberShipImageView"
                android:text="@string/current_membership"
                android:textSize="@dimen/_11ssp"
                android:layout_marginTop="8dp"
                android:id="@+id/currentMimberTextview"
                android:fontFamily="@font/poppins_medium"
                android:includeFontPadding="false"
                android:textColor="@color/toolbar_title_color"
                android:layout_marginStart="8dp"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Manage Subscription"
                android:textSize="@dimen/_9ssp"
                android:layout_marginTop="8dp"
                android:visibility="gone"
                android:id="@+id/MemberShipCancelBtnTextview"
                android:layout_alignParentRight="true"
                android:layout_below="@id/currentMimberTextview"
                android:fontFamily="@font/poppins_regular"
                android:includeFontPadding="false"
                android:textColor="@color/red"
                android:layout_marginStart="8dp"/>



            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_below="@id/currentMimberTextview"
                android:layout_toEndOf="@id/mimberShipImageView"
                android:layout_centerVertical="true"
                android:layout_marginStart="8dp"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Free Plan"
                    android:textSize="@dimen/_9ssp"
                    android:id="@+id/freePlanTextview"
                    android:layout_marginTop="5dp"
                    android:fontFamily="@font/poppins_regular"
                    android:textColor="@color/action_sub_text_color"
                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Advance Tools:"
                    android:textSize="@dimen/_9ssp"
                    android:id="@+id/freePlanTextviewPlan"
                    android:layout_marginStart="3dp"
                    android:layout_marginTop="5dp"
                    android:visibility="gone"
                    android:fontFamily="@font/poppins_regular"
                    android:textColor="@color/action_sub_text_color"
                    />


            </LinearLayout>







            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginStart="15dp"
                android:layout_alignParentBottom="true"
                android:background="@color/bg_color1"/>

        </RelativeLayout>


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/settings"
            android:textSize="@dimen/_13ssp"
            android:layout_marginTop="11dp"
            android:layout_marginStart="12dp"
            android:fontFamily="@font/poppins_semibold"
            android:textColor="@color/toolbar_title_color"

            />


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_marginTop="4dp"
            android:id="@+id/share_apps"

            >

            <ImageView
                android:id="@+id/iconImageView"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_alignParentStart="true"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:layout_marginBottom="8dp"
                android:src="@drawable/share_app_icon" />

            <TextView
                android:id="@+id/titleTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@id/iconImageView"
                android:text="@string/share_app"
                android:textSize="@dimen/_11ssp"
                android:fontFamily="@font/poppins_medium"
                android:textColor="@color/toolbar_title_color"
                android:layout_centerVertical="true"
                android:layout_marginStart="8dp"/>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginStart="15dp"
                android:layout_alignParentBottom="true"
                android:background="@color/bg_color1"/>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_marginTop="5dp"
            android:id="@+id/rate_us"
            >

            <ImageView
                android:id="@+id/iconImageView1"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_alignParentStart="true"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:background="@drawable/rate_us_icon_bg"
                android:layout_marginBottom="8dp"
                android:src="@drawable/rate_us_icon" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@id/iconImageView1"
                android:text="@string/rate_us"
                android:textSize="@dimen/_11ssp"
                android:fontFamily="@font/poppins_medium"
                android:textColor="@color/toolbar_title_color"
                android:layout_centerVertical="true"
                android:layout_marginStart="8dp"/>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginStart="15dp"
                android:layout_alignParentBottom="true"
                android:background="@color/bg_color1"/>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_marginTop="5dp"
            android:visibility="visible"
            android:id="@+id/language">

            <ImageView
                android:id="@+id/languageIcon"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_alignParentStart="true"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:layout_marginBottom="8dp"
                android:src="@drawable/language_icon" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@id/languageIcon"
                android:text="@string/language"
                android:textSize="@dimen/_11ssp"
                android:fontFamily="@font/poppins_medium"
                android:textColor="@color/toolbar_title_color"
                android:layout_centerVertical="true"
                android:layout_marginStart="8dp"/>

            <ImageView
                android:layout_width="60dp"
                android:layout_height="25dp"
                android:layout_alignParentEnd="true"
                android:layout_centerInParent="true"
                android:paddingEnd="8dp"
                android:src="@drawable/default_language_icon" />


            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginStart="15dp"
                android:layout_alignParentBottom="true"
                android:background="@color/bg_color1"/>

        </RelativeLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:visibility="gone"
            android:layout_marginTop="12dp"
            android:id="@+id/setMode"
            >

            <ImageView
                android:id="@+id/dark_Icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:paddingStart="8dp"
                android:paddingEnd="8dp"
                android:src="@drawable/dark_mode_icon" />



            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Change Mode"
                android:textSize="@dimen/_13ssp"
                android:layout_marginStart="10dp"
                android:textColor="@color/toolbar_title_color"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/dark_Icon"
                />



            <!--            <androidx.appcompat.widget.SwitchCompat-->
            <!--                android:layout_width="wrap_content"-->
            <!--                android:layout_height="wrap_content"-->
            <!--                android:checked="true"-->
            <!--                app:layout_constraintEnd_toEndOf="parent"-->
            <!--                app:layout_constraintTop_toTopOf="parent"-->
            <!--                android:id="@+id/themeSwitch"-->
            <!--                app:layout_constraintBottom_toBottomOf="parent"-->
            <!--                android:layout_marginEnd="8dp" />-->

            <androidx.appcompat.widget.SwitchCompat
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:checked="true"
                app:track="@drawable/track_selector_setting"
                android:thumb="@drawable/thumb_icon_setting"
                app:trackTint="@null"
                app:thumbTint="@null"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:id="@+id/themeSwitch"
                android:paddingEnd="8dp"
                app:layout_constraintBottom_toBottomOf="parent"
                 />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
